			</div>
		   	<input type="hidden" name="_csrf" value="{_csrf}" />
			<input type="hidden" name="type" id="type" value="0" />
			<input type="hidden" name="userid" id="userid" value="{userid}" />
			<input type="hidden" name="role" id="role" value="{role}" />
		</form>		


		<!-- if everything is filled out in the form, make a submit button on each page.
			 else if everything is filled out on that section, make a go to next section button
			 else button should be unclickable
			 also have to check on key up
		-->
		<script type="text/javascript">

			function makeHeaderUnclickable(booleanValue){
				if(booleanValue == "clickable"){
					$('#fakeHeader').hide();
				}
				else{
					$('#fakeHeader').show();
					$('#fakeHeader').click(function(){
						alert("Please complete profile before navigating away.");
					});
				}
			}

			function makeSubmitButton(){
				$('.btnbtn-success').prop('type','submit');
				$('.btnbtn-success').text('Update My Profile!');
				$('.btnbtn-success').css('background-color','green');
				$('.btnbtn-success').css('border-color','green');	
				$('.btnbtn-success').css('opacity','1');
				$('.btnbtn-success').css('cursor','pointer');	
				$(".btnbtn-success").attr("onclick","");	

				makeHeaderUnclickable("clickable");	
			}

			function undoSubmitButton(onPage, about, contact, education, industry, references, completionPercent){
				$('.btnbtn-success').prop('type','button');
				$('.btnbtn-success').css('background-color','#663333');
				$('.btnbtn-success').css('border-color','#663333');
				$('.btnbtn-success').css('opacity','.7');
				$('.btnbtn-success').css('cursor','default');	
				$('.btnbtn-success').text(completionPercent + "% Completed...");

				makeHeaderUnclickable("unclickable");
			}

			function percentComplete(education, education2, industry, industry2){
				var totalFields = 29;
				var totalCompleted = 0;
				if($('#fullName').val())
					totalCompleted++;
				if($('#preferredName').val())
					totalCompleted++;
				if($('.genderRadios').is(':checked') || $('.genderRadios2').is(':checked'))
					totalCompleted++;
				if($('#linkedinProfileUrl').val())
					totalCompleted++;
				if($('#coachingInterest').val())
					totalCompleted++;
				if($('#heardFrom').val())
					totalCompleted++;
				if($('#additionalPersonalInfo').val())
					totalCompleted++;

				if($('#email').val())
					totalCompleted++;
				if($('#phone').val())
					totalCompleted++;
				if($('#address').val())
					totalCompleted++;
				if($('#location').val())
					totalCompleted++;
				if($('.remoteLocationRadios').is(':checked') || $('.remoteLocationRadios2').is(':checked'))
					totalCompleted++;

				if(education2)
					totalCompleted++;
				if(education)
					totalCompleted++;
				if(industry)
					totalCompleted++;
				if(industry2)
					totalCompleted++;

				if($('#primaryIndustryJobTitle').val())
					totalCompleted++;
				if($('#primaryIndustryCompany').val())
					totalCompleted++;
				if($('#primaryIndustryYearsOfExperience').val())
					totalCompleted++;

				if($('#primaryReferenceName').val())
					totalCompleted++;
				if($('#primaryReferencePhone').val())
					totalCompleted++;
				if($('#primaryReferenceEmail').val())
					totalCompleted++;
				if($('#primaryReferenceRelationship').val())
					totalCompleted++;
				if($('#primaryReferenceYearsKnown').val())
					totalCompleted++;
				if($('#secondaryReferenceName').val())
					totalCompleted++;
				if($('#secondaryReferencePhone').val())
					totalCompleted++;
				if($('#secondaryReferenceEmail').val())
					totalCompleted++;
				if($('#secondaryReferenceRelationship').val())
					totalCompleted++;
				if($('#secondaryReferenceYearsKnown').val())
					totalCompleted++;

				return (totalCompleted/totalFields)*100;
			}


			function isComplete(){
				var about = false; var contact = false; var education = false; var industry = false; var references = false;
				if( 
					$('#fullName').val() && $('#preferredName').val() && ( $('.genderRadios').is(':checked') || $('.genderRadios2').is(':checked') ) && $('#linkedinProfileUrl').val() && $('#additionalPersonalInfo').val() && $('#coachingInterest').val() && $('#heardFrom').val() 
				){
					about = true;
					$('#about-status').text("About: Completed! "); $('#about-status').css('color','green'); $('#about-complete').addClass('fa fa-check');
				}
				else{
					about = false;
					$('#about-status').text("About: Something Missing...Go Here "); $('#about-status').css('color','black'); $('#about-complete').removeClass('fa fa-check');
				}

				if(
					$('#email').val() && $('#phone').val() && $('#address').val() && $('#location').val() && $('#zipCode').val() && ($('.remoteLocationRadios').is(':checked') || $('.remoteLocationRadios2').is(':checked'))
				){
					contact = true;
					$('#contact-status').text("Contact: Completed! "); $('#contact-status').css('color','green'); $('#contact-complete').addClass('fa fa-check');
				}
				else{
					contact = false;
					$('#contact-status').text("Contact: Something Missing...Go Here "); $('#contact-status').css('color','black'); $('#contact-complete').removeClass('fa fa-check');
				}

				var education2 = false;
				if( document.getElementById("underGradSchoolName").value != -1 && document.getElementById("underGradSchoolMajor").value != -1){
					education = true; education2 = true;
					if(document.getElementById("underGradSchoolName").value == "other"){
						if($('#otherUnderGradSchoolName').val()){
							education = true;
						}
						else{
							education = false;
						}
					}
					else{
						education = true;
					}
					if(document.getElementById("underGradSchoolMajor").value == "other"){
						if($('#otherUnderGradSchoolMajor').val()){
							education2 = true;
						}
						else{
							education2 = false;
						}
					}
					else{
						education2 = true;
					}
					if(education && education2){
						$('#education-status').text("Education: Completed! "); $('#education-status').css('color','green'); $('#education-complete').addClass('fa fa-check');					
					}
					else{
						$('#education-status').text("Education: Something Missing...Go Here "); $('#education-status').css('color','black'); $('#education-complete').removeClass('fa fa-check');
					}
				}
				else{
					education = false;
					$('#education-status').text("Education: Something Missing...Go Here "); $('#education-status').css('color','black'); $('#education-complete').removeClass('fa fa-check');
				}

				var industry2 = false;
				if (document.getElementById("primaryIndustryName").value != -1){
					industry = true; industry2 = true;
					if(document.getElementById("primaryIndustryName").value == "other"){
						if($('#otherPrimaryIndustryName').val()){
							industry2 = true;
						}
						else{
							industry2 = false;
							$('#industry-status').text("Industry: Something Missing...Go Here "); $('#industry-status').css('color','black'); $('#industry-complete').removeClass('fa fa-check');
						}
					}
					else{
						industry2 = true;
					}
					
					if($('#primaryIndustryJobTitle').val() && $('#primaryIndustryCompany').val() && $('#primaryIndustryYearsOfExperience').val()){
						industry = true;
					}
					else{
						industry = false;
						$('#industry-status').text("Industry: Something Missing...Go Here "); $('#industry-status').css('color','black'); $('#industry-complete').removeClass('fa fa-check');
					}

					if(industry && industry2){
						$('#industry-status').text("Industry: Completed! "); $('#industry-status').css('color','green'); $('#industry-complete').addClass('fa fa-check');						
					}
				}
				else{
					industry = false;
					$('#industry-status').text("Industry: Something Missing...Go Here "); $('#industry-status').css('color','black'); $('#industry-complete').removeClass('fa fa-check');
				}

				if( $('#primaryReferenceName').val() && $('#primaryReferencePhone').val() && $('#primaryReferenceEmail').val() && $('#primaryReferenceRelationship').val() && $('#primaryReferenceYearsKnown').val() && $('#secondaryReferenceName').val() && $('#secondaryReferencePhone').val() && $('#secondaryReferenceEmail').val() && $('#secondaryReferenceRelationship').val() && $('#secondaryReferenceYearsKnown').val() ){
					references = true;
					$('#references-status').text("References: Completed! "); $('#references-status').css('color','green'); $('#references-complete').addClass('fa fa-check');
				}
				else{
					references = false;
					$('#references-status').text("References: Something Missing...Go Here "); $('#references-status').css('color','black'); $('#references-complete').removeClass('fa fa-check');
				}

				if(about && contact && education && industry && references){
					makeSubmitButton();
				}
				else {
					var completionPercent = Math.round(percentComplete(education, education2, industry, industry2));
					undoSubmitButton(onPage, about, contact, education, industry, references, completionPercent);
				}

			}

			$( document ).ready(function() {
			    isComplete();
			});

		</script>
	{/data.result}
{/body}

